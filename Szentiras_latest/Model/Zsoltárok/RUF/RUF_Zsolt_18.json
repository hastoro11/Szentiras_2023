                "hely": {
                    "gepi": 12101802000,
                    "szep": "Zsolt 18,20"
                },
                "jegyzetek": [],
                "szoveg": "Igazságom szerint bánt velem az ÚR, kezem tisztasága szerint jutalmazott engem."
