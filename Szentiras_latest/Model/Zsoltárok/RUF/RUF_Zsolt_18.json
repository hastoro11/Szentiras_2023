            {
                "hely": {
                    "gepi": 12101800300,
                    "szep": "Zsolt 18,3"
                },
                "jegyzetek": [],
                "szoveg": "Az ÚRhoz kiáltok, aki dicséretre méltó, és megszabadulok ellenségeimtől."
            },
            {
                "hely": {
                    "gepi": 12101800400,
                    "szep": "Zsolt 18,4"
                },
                "jegyzetek": [],
                "szoveg": "Körülvettek a halál kötelei, pusztító áradat rettent engem."
            },
            {
                "hely": {
                    "gepi": 12101800500,
                    "szep": "Zsolt 18,5"
                },
                "jegyzetek": [],
                "szoveg": "A sír kötelei fonódtak rám, a halál csapdái meredtek rám."
