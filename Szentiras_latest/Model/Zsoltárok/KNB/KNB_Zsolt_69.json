                "jegyzetek": [
                    {
                        "position": null,
                        "text": "Egy ártatlan üldözött panasza a fogság idejéből."
                    }
                ],
                "szoveg": "<em>A karvezetőnek. A »Liliomok« szerint. Dávidtól.</em><br>"
