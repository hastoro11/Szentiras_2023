{
    "keres": {
        "feladat": "idezet",
        "forma": "json",
        "hivatkozas": "Zsolt 90"
    },
    "valasz": {
        "forditas": {
            "nev": "Szent István Társulati Biblia",
            "rov": "SZIT"
        },
        "versek": [
            {
                "hely": {
                    "gepi": 12109000000,
                    "szep": "Zsolt 90,0"
                },
                "jegyzetek": [],
                "szoveg": "(Mózesnek, az Isten emberének imája.)"
            },
            {
                "hely": {
                    "gepi": 12109000100,
                    "szep": "Zsolt 90,1"
                },
                "jegyzetek": [],
                "szoveg": "Uram, te voltál menedékünk nemzedékről nemzedékre,"
            },
            {
                "hely": {
                    "gepi": 12109000200,
                    "szep": "Zsolt 90,2"
                },
                "jegyzetek": [],
                "szoveg": "mielőtt a hegyek kiemelkedtek, mielőtt a föld és a világ kialakult. Isten, te öröktől fogva vagy, s örökké!"
            },
            {
                "hely": {
                    "gepi": 12109000300,
                    "szep": "Zsolt 90,3"
                },
                "jegyzetek": [],
                "szoveg": "Te a halandót ismét porrá teszed. Így szólsz: „Emberek fiai, térjetek oda vissza!”"
            },
            {
                "hely": {
                    "gepi": 12109000400,
                    "szep": "Zsolt 90,4"
                },
                "jegyzetek": [],
                "szoveg": "Mert ezer év előtted annyi, mint a tegnapi nap, amely elmúlt. Vagy annyi, mint egy éjjeli őrállás."
            },
            {
                "hely": {
                    "gepi": 12109000500,
                    "szep": "Zsolt 90,5"
                },
                "jegyzetek": [],
                "szoveg": "Hamar elragadod őket, olyanok, mint a reggeli álom, mint a sarjadó fű:"
            },
            {
                "hely": {
                    "gepi": 12109000600,
                    "szep": "Zsolt 90,6"
                },
                "jegyzetek": [],
                "szoveg": "reggel kihajt, kizöldül, estére lekaszálják és elszárad."
            },
            {
                "hely": {
                    "gepi": 12109000700,
                    "szep": "Zsolt 90,7"
                },
                "jegyzetek": [],
                "szoveg": "Valóban semmivé leszünk haragod előtt, megzavarodunk neheztelésedtől."
            },
            {
                "hely": {
                    "gepi": 12109000800,
                    "szep": "Zsolt 90,8"
                },
                "jegyzetek": [],
                "szoveg": "A szemed elé állítottad bűneinket, arcod világosságában látod vétkeinket."
            },
            {
                "hely": {
                    "gepi": 12109000900,
                    "szep": "Zsolt 90,9"
                },
                "jegyzetek": [],
                "szoveg": "Minden napunk haragodban múlt el, éveinket egyetlen sóhajként éltük át."
            },
            {
                "hely": {
                    "gepi": 12109001000,
                    "szep": "Zsolt 90,10"
                },
                "jegyzetek": [],
                "szoveg": "Éveinknek száma legföljebb hetven, s ha erősek vagyunk, eljutunk nyolcvanig. Ebből is a legtöbb betegség, hiú fáradozás; gyorsan elszállnak, hamar végük szakad."
            },
            {
                "hely": {
                    "gepi": 12109001100,
                    "szep": "Zsolt 90,11"
                },
                "jegyzetek": [],
                "szoveg": "Ki mérheti föl haragod erejét, ki fél eléggé haragod hevétől?"
            },
            {
                "hely": {
                    "gepi": 12109001200,
                    "szep": "Zsolt 90,12"
                },
                "jegyzetek": [],
                "szoveg": "Taníts meg számba venni napjainkat, hogy eljussunk a szív bölcsességére!"
            },
            {
                "hely": {
                    "gepi": 12109001300,
                    "szep": "Zsolt 90,13"
                },
                "jegyzetek": [],
                "szoveg": "Fordulj felénk újra, Uram! Miért késel oly soká? Légy irgalmas szolgáidhoz,"
            },
            {
                "hely": {
                    "gepi": 12109001400,
                    "szep": "Zsolt 90,14"
                },
                "jegyzetek": [],
                "szoveg": "üdíts fel minket irgalmaddal, hogy ujjongjunk és örüljünk ezentúl minden nap."
            },
            {
                "hely": {
                    "gepi": 12109001500,
                    "szep": "Zsolt 90,15"
                },
                "jegyzetek": [],
                "szoveg": "Vidíts fel a napokért, amelyeken próbára tettél, az évekért, amelyekben nyomorogtunk."
            },
            {
                "hely": {
                    "gepi": 12109001600,
                    "szep": "Zsolt 90,16"
                },
                "jegyzetek": [],
                "szoveg": "Nyilvánítsd ki művedet szolgáidnak, s gyermekeik előtt tárd fel dicsőséged!"
            },
            {
                "hely": {
                    "gepi": 12109001700,
                    "szep": "Zsolt 90,17"
                },
                "jegyzetek": [],
                "szoveg": "Az Úr jósága virrasszon fölöttünk, őrizd meg a kezünk munkáját!"
            }
        ]
    }
}
