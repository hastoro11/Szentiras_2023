        "versek": [
            {
                "hely": {
                    "gepi": 12109200000,
                    "szep": "Zsolt 92,0"
                },
                "jegyzetek": [],
                "szoveg": "(Zsoltár, szombati ének.)"
            },
            {
                "hely": {
                    "gepi": 12109200100,
                    "szep": "Zsolt 92,1"
                },
                "jegyzetek": [],
                "szoveg": "Jó dolog az Urat dicsérni, nevednek, ó Fölséges, éneket zengeni;"
            },
            {
                "hely": {
                    "gepi": 12109200200,
                    "szep": "Zsolt 92,2"
                },
                "jegyzetek": [],
                "szoveg": "reggel hirdetni irgalmadat, éjszaka meg hűségedet,"
            },
            {
                "hely": {
                    "gepi": 12109200300,
                    "szep": "Zsolt 92,3"
                },
                "jegyzetek": [],
                "szoveg": "tízhúrú hárfával és lanttal, énekkel és muzsikával."
            },
            {
                "hely": {
                    "gepi": 12109200400,
                    "szep": "Zsolt 92,4"
                },
                "jegyzetek": [],
                "szoveg": "Tetteid, Uram, örömmel töltenek el, kezed művein ujjongok:"
            },
            {
                "hely": {
                    "gepi": 12109200500,
                    "szep": "Zsolt 92,5"
                },
                "jegyzetek": [],
                "szoveg": "„Mily fönségesek a tetteid, Uram, s gondolataid nem lehet kifürkészni!”"
            },
            {
                "hely": {
                    "gepi": 12109200600,
                    "szep": "Zsolt 92,6"
                },
                "jegyzetek": [],
                "szoveg": "Az értelmetlen ember nem foghatja fel, a balga nem ismeri meg őket soha."
            },
            {
                "hely": {
                    "gepi": 12109200700,
                    "szep": "Zsolt 92,7"
                },
                "jegyzetek": [],
                "szoveg": "A bűnösök nőhetnek, mint a fű, a gonoszok csillogva járhatnak: örök pusztulásra vannak ítélve mind,"
            },
            {
                "hely": {
                    "gepi": 12109200800,
                    "szep": "Zsolt 92,8"
                },
                "jegyzetek": [],
                "szoveg": "de te fölséges vagy örökkén-örökké."
            },
            {
                "hely": {
                    "gepi": 12109200900,
                    "szep": "Zsolt 92,9"
                },
                "jegyzetek": [],
                "szoveg": "Lám, ellenségeidnek el kell pusztulniuk, a gonosztevőket szétszórják mind, de belém erőt öntesz."
            },
            {
                "hely": {
                    "gepi": 12109201000,
                    "szep": "Zsolt 92,10"
                },
                "jegyzetek": [],
                "szoveg": "Fejem fölemelted, mint a bivaly szarvát, és fölkentél színtiszta olajjal."
            },
            {
                "hely": {
                    "gepi": 12109201100,
                    "szep": "Zsolt 92,11"
                },
                "jegyzetek": [],
                "szoveg": "Szemem letekint ellenségeimre, fülem hallja a gonoszokat."
            },
            {
                "hely": {
                    "gepi": 12109201200,
                    "szep": "Zsolt 92,12"
                },
                "jegyzetek": [],
                "szoveg": "Az igaz virul, mint a pálma, fölfelé nő, mint a Libanon cédrusa."
            },
            {
                "hely": {
                    "gepi": 12109201300,
                    "szep": "Zsolt 92,13"
                },
                "jegyzetek": [],
                "szoveg": "Az Úr házában vannak elültetve, Istenünk csarnokában virágoznak."
            },
            {
                "hely": {
                    "gepi": 12109201400,
                    "szep": "Zsolt 92,14"
                },
                "jegyzetek": [],
                "szoveg": "Még öregkorukban is gyümölcsöt teremnek, tele vannak nedvvel és élettel,"
            },
            {
                "hely": {
                    "gepi": 12109201500,
                    "szep": "Zsolt 92,15"
                },
                "jegyzetek": [],
                "szoveg": "hogy hirdessék: „Az Úr igazságos ő az én sziklám, igazságtalanság nincsen benne!”"
            }
        ]
    }
}