            {
                "hely": {
                    "gepi": 12108300100,
                    "szep": "Zsolt 83,1"
                },
                "jegyzetek": [],
                "szoveg": "Istenem, ne maradj néma, ne hallgass, Istenem, ne légy tétlen!"
            },
            {
                "hely": {
                    "gepi": 12108300200,
                    "szep": "Zsolt 83,2"
                },
                "jegyzetek": [],
                "szoveg": "Nézd, hogyan dühöngnek ellenségeid, s milyen fenn hordják a fejüket, akik gyűlölnek téged."
            },
            {
                "hely": {
                    "gepi": 12108300300,
                    "szep": "Zsolt 83,3"
                },
                "jegyzetek": [],
                "szoveg": "Gonoszat forralnak néped ellen, védetted ellen tanácsot tartanak."
            },
            {
                "hely": {
                    "gepi": 12108300400,
                    "szep": "Zsolt 83,4"
                },
                "jegyzetek": [],
                "szoveg": "Így szólnak: „Gyertek, töröljük ki a népek sorából, Izrael nevét ne is emlegessék!”"
            },
            {
                "hely": {
                    "gepi": 12108300500,
                    "szep": "Zsolt 83,5"
                },
                "jegyzetek": [],
                "szoveg": "Igen, egyetértőn tanácskoznak, és szövetkeznek ellened:"
            },
            {
                "hely": {
                    "gepi": 12108300600,
                    "szep": "Zsolt 83,6"
                },
                "jegyzetek": [],
                "szoveg": "Edom sátrai, az izmaeliták, Moáb és a hagriták,"
            },
            {
                "hely": {
                    "gepi": 12108300700,
                    "szep": "Zsolt 83,7"
                },
                "jegyzetek": [],
                "szoveg": "Gebál, Ammon és Amalek, a filiszteusok és Tírusz lakói."
            },
            {
                "hely": {
                    "gepi": 12108300800,
                    "szep": "Zsolt 83,8"
                },
                "jegyzetek": [],
                "szoveg": "Asszur is velük szövetkezett, segítséget nyújt Lót fiainak."
            },
            {
                "hely": {
                    "gepi": 12108300900,
                    "szep": "Zsolt 83,9"
                },
                "jegyzetek": [],
                "szoveg": "Bánj velük, ahogy Midiánnal tettél, Sziszerával és Jabinnal a Kison-pataknál."
            },
            {
                "hely": {
                    "gepi": 12108301000,
                    "szep": "Zsolt 83,10"
                },
                "jegyzetek": [],
                "szoveg": "Endornál megsemmisültek, és a földben televénnyé lettek."
            },
            {
                "hely": {
                    "gepi": 12108301100,
                    "szep": "Zsolt 83,11"
                },
                "jegyzetek": [],
                "szoveg": "Fejedelmeikkel bánj el, mint Orebbel és Zebbel, vezéreikkel tégy úgy, ahogy Szebachhal és Zalmunnával,"
            },
            {
                "hely": {
                    "gepi": 12108301200,
                    "szep": "Zsolt 83,12"
                },
                "jegyzetek": [],
                "szoveg": "akik azt mondogatták: „Szerezzük meg magunknak Izrael földjeit!”"
            },
            {
                "hely": {
                    "gepi": 12108301300,
                    "szep": "Zsolt 83,13"
                },
                "jegyzetek": [],
                "szoveg": "Istenem, tedd őket olyanná, mint a száraz kóró, mint pelyva a szélben,"
            },
            {
                "hely": {
                    "gepi": 12108301400,
                    "szep": "Zsolt 83,14"
                },
                "jegyzetek": [],
                "szoveg": "mint a tűz, mely égeti az erdőt, mint a láng, mely nyaldossa a hegyet!"
            },
            {
                "hely": {
                    "gepi": 12108301500,
                    "szep": "Zsolt 83,15"
                },
                "jegyzetek": [],
                "szoveg": "Így hajszold őket a viharban, fenyegesd meg őket zivatarodban!"
            },
            {
                "hely": {
                    "gepi": 12108301600,
                    "szep": "Zsolt 83,16"
                },
                "jegyzetek": [],
                "szoveg": "Arcukat borítsa gyalázat, és keressék a te nevedet, Uram!"
            },
            {
                "hely": {
                    "gepi": 12108301700,
                    "szep": "Zsolt 83,17"
                },
                "jegyzetek": [],
                "szoveg": "Érje őket örök szégyen, tanácstalanság, pusztuljanak el dicstelenül!"
            },
            {
                "hely": {
                    "gepi": 12108301800,
                    "szep": "Zsolt 83,18"
                },
                "jegyzetek": [],
                "szoveg": "Hadd ismerjenek meg téged, akinek neve: Úr! Az egész világon egyedül te vagy fölséges."
            }
        ]
    }
