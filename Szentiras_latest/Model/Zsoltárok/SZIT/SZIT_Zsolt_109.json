            {
                "hely": {
                    "gepi": 12110902000,
                    "szep": "Zsolt 109,20"
                },
                "jegyzetek": [],
                "szoveg": "Ez legyen jutalma az Úrtól azoknak, akik vádolnak engem, akik gonoszul szólnak ellenem!"
